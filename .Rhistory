check.window <- function(vect, per){
wind <- per[1]:per[2]
wind <- glue_collapse(as.character(wind), sep = ",|,")
wind <- paste(",", wind, ",", sep = "")
ifelse(grepl(pattern = wind, x = vect), 1, 0)
}
